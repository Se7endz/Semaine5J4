<h1>Bienvenue dans le formulaire de connexion</h1>
<br>
<br>
<br>
<h3>Veuillez remplir les champs ci-dessous</h3>

<%= form_tag sessions_path do |f| %>
	<div class="form-group" style="width: 18rem">
		<label>Email</label>
		<%= text_field_tag :email %>
	</div>
	<div class="form-group" style="width: 18rem">
		<label>Mot de passe</label>
		<%= password_field_tag :password %>
	</div>
	<div>
		<%= submit_tag("Validez") %>
	</div>
	<% end %>